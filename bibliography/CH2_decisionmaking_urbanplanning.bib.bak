@article{roy_urban_2005,
	title = {Urban {Informality}: {Toward} an {Epistemology} of {Planning}},
	volume = {71},
	issn = {0194-4363},
	shorttitle = {Urban {Informality}},
	url = {http://www.tandfonline.com/doi/abs/10.1080/01944360508976689},
	doi = {10.1080/01944360508976689},
	abstract = {Abstract Many of the significant urban transformations of the new century are taking place in the developing world. In particular, informality, once associated with poor squatter settlements, is now seen as a generalized mode of metropolitan urbanization. This article focuses on urban informality to highlight the challenges of dealing with the “unplannable” exceptions to the order of formal urbanization. It argues that planners must learn to work with this state of exception. Such policy epistemologies are useful not only for “Third World” cities but also more generally for urban planning concerned with distributive justice.},
	number = {2},
	urldate = {2013-09-20},
